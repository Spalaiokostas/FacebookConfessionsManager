<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="207.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="anonPostManager.view.ConfigPropertiesController">
   <header>
      <Label alignment="CENTER" text="Config Properties">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </header>
   <content>
      <TabPane prefHeight="20.0" prefWidth="394.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Form Properties">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="141.0" prefWidth="347.0">
                     <children>
                        <Label layoutX="14.0" layoutY="14.0" text="Form Response Filename" />
                        <Label layoutX="14.0" layoutY="41.0" text="Form Response Format Type" />
                        <ChoiceBox fx:id="formType" layoutX="208.0" layoutY="10.0" prefWidth="150.0" />
                        <TextField fx:id="formName" layoutX="208.0" layoutY="37.0" />
                        <Button layoutX="305.0" layoutY="80.0" mnemonicParsing="false" text="Cancel" />
                        <Button layoutX="267.0" layoutY="80.0" mnemonicParsing="false" text="ok" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Anomologita Properties">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="141.0" prefWidth="347.0">
                     <children>
                        <Label layoutX="14.0" layoutY="11.0" text="Anomologita Number Refresh Rate" />
                        <Label layoutX="14.0" layoutY="45.0" text="HashTag Refresh Rate" />
                        <TextField fx:id="responseLimit" layoutX="208.0" layoutY="10.0" />
                        <TextField fx:id="hashtagLimit" layoutX="208.0" layoutY="37.0" />
                        <Button layoutX="305.0" layoutY="80.0" mnemonicParsing="false" text="Cancel" />
                        <Button layoutX="267.0" layoutY="80.0" mnemonicParsing="false" text="ok" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </content>
</DialogPane>
