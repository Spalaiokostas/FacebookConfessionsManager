<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="133.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="confessionsManager.view.InitConfigFormPropertiesController">
   <content>
      <AnchorPane prefHeight="104.0" prefWidth="461.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Select name of the Form Response File">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="40.0" text="Select type of the Form Response File">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="formTypeList" layoutX="253.0" layoutY="36.0" prefWidth="150.0" />
            <TextField fx:id="formName" layoutX="253.0" layoutY="10.0" />
            <CheckBox fx:id="remeberCheckBox" layoutX="230.0" layoutY="76.0" mnemonicParsing="false" text="Remember Me !">
               <font>
                  <Font name="System Italic" size="11.0" />
               </font>
            </CheckBox>
            <Label layoutX="23.0" layoutY="97.0" text="*Tip: You can later change the above properties from menu: Tools-&gt;Properties" />
            <Button fx:id="chooseButton" layoutX="120.0" layoutY="72.0" mnemonicParsing="false" text="Save Properties" underline="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </content>
   <header>
      <Label alignment="CENTER" text="Configure Form Properties">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </header>
</DialogPane>
